{
  "name": "loomii-geo-backend",
  "version": "1.0.0",
  "description": "Express TypeScript backend server with Prisma ORM for Loomii Geo application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx src/utils/seed.ts",
    "db:clear": "tsx src/utils/clear.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:daily": "tsx src/tests/dailyJob.ts",
    "test:analytics": "tsx src/tests/testAnalytics.ts",
    "test:competitors": "tsx src/tests/testCompetitors.ts",
    "test:sources": "tsx src/tests/testSources.ts",
    "test:api": "npm run test:analytics && npm run test:competitors && npm run test:sources",
    "seed:tags": "tsx src/utils/seedTags.ts",
    "migrate:dates": "tsx src/utils/migratePromptRunDates.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "postgresql"
  ],
  "author": "Loomii",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/google-vertex": "^2.2.27",
    "@ai-sdk/openai": "^1.3.22",
    "@prisma/client": "^5.6.0",
    "ai": "^4.3.16",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
